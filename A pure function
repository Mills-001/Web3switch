pragma solidity  0.8.7;

contract Crudeoil {
    function processOil(uint _barrels, uint _quality) public pure returns(uint) {
        uint refiningCost = _barrels * 15;
        uint qualityBonus = _quality * 2;
        uint totalValue = (refiningCost + qualityBonus);
        return totalValue;
        }

}
